		/**
		 * 获取指定中文字符的使用频率
		 * @param	val   要获取的字符(只取第一个字符)
		 * @return  int   该字符的使用频率(从0开始，-1表示字表中没有这个字符)
		 */
		public static function getRatio(val:String):int
		{
			if (!val || ! val.length) return -1;
			val = val.charAt();
			var numCharacterList:int = characterList.length;
			var characters:String;
			for (var i:int = 0; i < numCharacterList; i++)
			{
				characters = characterList[i];
				if (characters.indexOf(val) != -1)
				{
					return i;
				}
			}
			return -1;
		}
		
